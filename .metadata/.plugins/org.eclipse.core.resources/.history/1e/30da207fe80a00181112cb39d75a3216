	<!-- namespace的值一定是接口的全路径与接口绑定后,此文件相当于接口的实现类  -->
<mapper namespace="com.gqw.dao.order.MyOrderMapper">
<insert id="insertOrder" parameterType="Order">
	insert into dingdan(ordernumber,name,numbers,money,ordertime,shouhuoren,shouhuoaddress,phonenumber,status,wuliugongsi,wuliudanhao,chaxunaddress)
	values(#{ordernumber},#{name},#{numbers},#{money},#{ordertime},#{shouhuoren},#{shouhuoaddress},#{phonenumber},#{status},#{wuliugongsi},#{wuliudanhao},#{chaxunaddress});		
</insert>
</mapper>